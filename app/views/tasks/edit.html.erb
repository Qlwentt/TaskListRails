<h1>Tasks#edit</h1>
<h1>Update a Task</h1>
<form name="edit-task" action="./update" method="post">
	<%= form_for @my_task, url: update_path do |f| %>
		<%= f.label :title %>
		<%= f.text_field :title %>

		<%= f.label :description %>
		<%= f.text_field :description %>

		<%= f.label :completed_at %>
		<%= f.text_field :completed_at %>

		<%= f.submit %>



	<% end %>
